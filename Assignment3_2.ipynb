    "def reverse_string(input_string):\n",
    "    return input_string[::-1]\n",
    "\n",
    "# Taking user input\n",
    "input_str = input(\"Enter a string: \")\n",
    "\n",
    "reversed_str = reverse_string(input_str)\n",
    "print(\"Reversed string:\", reversed_str)\n"
